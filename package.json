{
  "name": "diva-e-cypress",
  "displayName": "diva-e-cypress",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "diva-e-cypress.generateTest",
        "title": "Generate Cypress Test (Orchestrator)"
      },
      {
        "command": "diva-e-cypress.generateCodeLlama",
        "title": "Generate Cypress Test (CodeLLama)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "diva-e-cypress.generateTest",
          "when": "resourceExtname == .feature",
          "group": "navigation"
        },
        {
          "command": "diva-e-cypress.generateCodeLlama",
          "when": "resourceExtname == .feature",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "build":   "tsc -p .",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@cucumber/gherkin": "^32.1.2",
    "@cucumber/messages": "^27.2.0",
    "@huggingface/inference": "^4.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.4",
    "eslint": "^9.25.1",
    "inference": "^2.0.106",
    "ollama": "^0.5.15",
    "puppeteer": "^24.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "node-fetch": "^2.7.0",
    "langchain": "^0.3.30",
    "@langchain/langgraph": "^0.3.11",
    "@langchain/core": "^0.3.30", 
    "@langchain/ollama": "^0.2.3"
  }
}
